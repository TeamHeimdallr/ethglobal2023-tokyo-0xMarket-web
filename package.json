{
  "name": "0xmarket-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "pm2-dev ./pm2-mock.config.js",
    "start:local": "pm2-dev ./pm2-local.config.js",
    "start:storybook": "pm2-dev ./pm2-storybook.config.js",
    "build:dev": "dotenv -e .env.dev -- craco build",
    "build:css": "postcss ./src/styles/base/index.css --output ./src/styles/build/index.css",
    "clean": "rm -rf build",
    "check:lint": "eslint src --ext ts --ext tsx --fix",
    "check:prettier": "prettier --write --ignore-path .gitignore \"**/*.+(js|jsx|ts|tsx|json)\"",
    "check:type": "tsc --p ./tsconfig.json --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "@emotion/react": "^11.10.6",
    "@hookform/resolvers": "^3.0.1",
    "@loadable/component": "^5.15.3",
    "@tanstack/react-query": "^4.28.0",
    "axios": "^1.3.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.2.1",
    "ethers": "^5",
    "framer-motion": "^10.11.2",
    "graphql-request": "^5.2.0",
    "immer": "^9.0.21",
    "lodash-es": "^4.17.21",
    "lottie-web": "^5.10.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.3",
    "react-number-format": "^5.1.4",
    "react-responsive": "^9.0.2",
    "react-router-dom": "^6.10.0",
    "react-scripts": "^5.0.1",
    "siwe": "^1.1.6",
    "tinycolor2": "^1.6.0",
    "usehooks-ts": "^2.9.1",
    "wagmi": "^0.12.9",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@faker-js/faker": "^7.6.0",
    "@mswjs/data": "^0.12.0",
    "@storybook/addon-actions": "^7.0.2",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-interactions": "^7.0.2",
    "@storybook/addon-links": "^7.0.2",
    "@storybook/addon-viewport": "^7.0.2",
    "@storybook/blocks": "^7.0.2",
    "@storybook/preset-create-react-app": "^7.0.2",
    "@storybook/react": "^7.0.2",
    "@storybook/react-webpack5": "^7.0.2",
    "@tailwindcss/forms": "^0.5.3",
    "@types/loadable__component": "^5.13.4",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.15.11",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/tinycolor2": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.5.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "cssnano": "^6.0.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "msw": "^1.2.1",
    "msw-storybook-addon": "^1.8.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "pm2": "^5.3.0",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^8.3.0",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.2",
    "tailwindcss": "^3.3.1",
    "tailwindcss-truncate-multiline": "^1.0.3",
    "ts-jest": "^29.1.0",
    "twin.macro": "^3.3.0",
    "typescript": "^5.0.3",
    "webpack": "^5.78.0",
    "webpackbar": "^5.0.2"
  },
  "resolutions": {
    "@types/react": "18.0.0",
    "@types/react-dom": "18.0.3"
  },
  "babelMacros": {
    "twin": {
      "preset": "emotion"
    }
  },
  "msw": {
    "workerDirectory": "public"
  },
  "packageManager": "yarn@3.5.0",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
